    "@slack/web-api": "^6.7.2",
    "@superfaceai/one-sdk": "^1.3.0",
    "@superfaceai/passport-twitter-oauth2": "^1.0.0",
    "analytics-node": "^6.2.0",
    "aws-sdk": "2.814.0",
    "axios": "^0.27.2",
    "bcrypt": "5.0.0",
    "body-parser": "^1.20.1",
    "bunyan": "^1.8.15",
