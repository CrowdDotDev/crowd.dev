{
  "name": "frontend",
  "version": "0.5.0",
  "private": true,
  "scripts": {
    "lint": "vue-cli-service lint",
    "build:localhost": "vue-cli-service build --mode localhost",
    "build:production": "vue-cli-service build --mode prod",
    "build:staging": "vue-cli-service build --mode staging",
    "start": "vue-cli-service serve --mode localhost --port 8081",
    "start:dev": "vue-cli-service serve --mode localhost --port 8081",
    "start:dev:local": "set -a && . ./.env.dist.local && . ./.env.override.local && set +a && vue-cli-service serve --mode localhost --port 8081",
    "test": "node tests/localhost-runner.js",
    "test:e2e:open": "wait-on http://localhost:8081 && npx cypress open",
    "test:e2e:run": "wait-on http://localhost:8081 && npx cypress run",
    "test:e2e:staging:open": "npx cypress open --config-file cypress.staging.config.js",
    "test:e2e:staging:run": "npx cypress run --config-file cypress.staging.config.js",
    "tailwind:view": "tailwind-config-viewer -o"
  },
  "dependencies": {
    "@cubejs-client/core": "^0.31.0",
    "@cubejs-client/vue3": "^0.31.0",
    "@nangohq/pizzly-frontend": "^0.4.0",
    "@octokit/core": "^3.5.1",
    "@tailwindcss/postcss7-compat": "^2.2.17",
    "@tiptap/extension-placeholder": "^2.0.0-beta.199",
    "@tiptap/starter-kit": "^2.0.0-beta.199",
    "@tiptap/vue-3": "^2.0.0-beta.199",
    "@vueuse/core": "^9.7.0",
    "apollo-boost": "^0.4.9",
    "axios": "^0.22.0",
    "chart.js": "^3.7.0",
    "chartjs-adapter-moment": "^1.0.0",
    "chartkick": "mariobalca/chartkick.js.git",
    "core-js": "3.24.1",
    "element-plus": "^2.2.19",
    "emoji-toolkit": "^6.6.0",
    "file-saver": "2.0.2",
    "filesize": "6.1.0",
    "graphql": "^15.6.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "logrocket": "^3.0.1",
    "logrocket-vuex": "^0.0.3",
    "marked": "^4.2.3",
    "md5": "2.3.0",
    "moment": "^2.29.4",
    "nprogress": "0.2.0",
    "pluralize": "^8.0.0",
    "posthog-js": "^1.36.1",
    "qs": "6.9.4",
    "regenerator-runtime": "^0.13.9",
    "remixicon": "^2.5.0",
    "socket.io-client": "^4.5.4",
    "uuid": "8.3.0",
    "vue": "^3.2.45",
    "vue-3-sanitize": "^0.1.4",
    "vue-grid-layout": "3.0.0-beta1",
    "vue-json-pretty": "^2.2.2",
    "vue-router": "^4.1.5",
    "vuedraggable": "^4.1.0",
    "vuex": "^4.0.2",
    "xlsx": "^0.17.2",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@tiptap/extension-link": "^2.0.0-beta.202",
    "@vue/cli-plugin-babel": "^5.0.8",
    "@vue/cli-plugin-eslint": "^5.0.1",
    "@vue/cli-service": "^5.0.1",
    "@vue/compiler-sfc": "^3.2.29",
    "@vue/eslint-config-prettier": "^7.0.0",
    "autoprefixer": "^10.4.2",
    "concurrently": "^7.0.0",
    "cypress": "^10.7.0",
    "cypress-mailosaur": "^2.13.0",
    "dotenv": "^16.0.3",
    "eslint": "8.22.0",
