{
  "name": "frontend",
  "version": "0.5.0",
  "private": true,
  "scripts": {
    "lint": "eslint \"{src,tests}/**/*.{js,ts,vue}\" --max-warnings=0",
    "lint:fix": "npm run lint -- --fix",
    "build:localhost": "vite build --mode localhost",
    "build:production": "vite build --mode prod",
    "build:staging": "vite build --mode staging",
    "start": "vite --host",
    "start:dev": "vite --host",
    "start:dev:local": "set -a && . ./.env.dist.local && . ./.env.override.local && set +a && vite --mode localhost --port 8081",
    "test": "node tests/localhost-runner.js",
    "test:e2e:open": "npx cypress open",
    "test:e2e:run": "npx cypress run --headed --browser chrome",
    "test:e2e:staging:open": "npx cypress open --config-file cypress.staging.config.js",
    "test:e2e:staging:run": "npx cypress run --config-file cypress.staging.config.js",
    "docs:tailwind": "tailwind-config-viewer -o",
    "docs:storybook": "storybook dev -p 6006",
    "docs:storybook:build": "storybook build",
    "docs": "npm run docs:tailwind & npm run docs:storybook"
  },
  "lint-staged": {
    "**/*.{js,ts,vue}": [
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@auth0/auth0-spa-js": "^2.1.3",
    "@datadog/browser-rum": "^5.9.0",
    "@formbricks/js": "1.2.0",
    "@linuxfoundation/lfx-ui-core": "^0.0.20",
    "@nangohq/frontend": "^0.9.0",
    "@nangohq/frontend-v2": "npm:@nangohq/frontend@^0.52.4",
    "@tailwindcss/line-clamp": "^0.4.2",
    "@tanstack/vue-query": "^5.75.1",
    "@tanstack/vue-query-devtools": "^5.75.1",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vue/eslint-config-airbnb": "^7.0.0",
    "@vuelidate/core": "^2.0.1",
    "@vuelidate/validators": "^2.0.1",
    "@vueuse/core": "^9.13.0",
    "axios": "^1.12.0",
    "chart.js": "^3.9.1",
    "chartjs-adapter-moment": "^1.0.1",
    "dayjs": "^1.11.13",
    "element-plus": "^2.11.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.27.5",
    "lodash": "^4.17.21",
    "logrocket": "^3.0.1",
    "logrocket-vuex": "^0.0.3",
    "marked": "^4.3.0",
    "node-emoji": "^1.11.0",
    "nprogress": "0.2.0",
    "pinia": "^2.0.33",
    "pluralize": "^8.0.0",
    "qs": "^6.11.1",
    "socket.io-client": "^4.6.1",
    "vite": "^5.4.21",
    "vue": "^3.3.1",
    "vue-3-sanitize": "^0.1.4",
    "vue-draggable-next": "^2.2.1",
    "vue-grid-layout": "3.0.0-beta1",
    "vue-router": "^4.1.6",
    "vue3-click-away": "^1.2.4",
    "vue3-lazyload": "^0.3.6",
    "vuex": "^4.1.0",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^7.6.10",
    "@storybook/addon-interactions": "^7.6.10",
    "@storybook/addon-links": "^7.6.10",
    "@storybook/vue3": "^7.6.10",
    "@storybook/vue3-vite": "^7.6.10",
    "@types/pluralize": "^0.0.33",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "autoprefixer": "^10.4.14",
    "cypress": "^13.7.3",
    "cypress-mailosaur": "^2.13.0",
    "dotenv": "^16.0.3",
    "eslint": "^8.36.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-storybook": "^0.6.15",
    "eslint-plugin-vue": "^9.10.0",
    "lint-staged": "^15.4.3",
    "postcss": "^8.4.21",
    "rollup-plugin-visualizer": "^5.9.2",
    "sass": "^1.80.0",
    "storybook": "^7.6.10",
    "tailwind-config-viewer": "^1.7.2",
    "tailwindcss": "^3.2.7",
    "typescript": "~5.0.4",
    "unplugin-auto-import": "^0.15.2",
    "unplugin-vue-components": "^0.24.1"
  }
}
