    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-vue": "^8.4.1",
    "postcss": "^8.4.6",
    "prettier": "^2.1.2",
    "sass": "^1.49.7",
    "sass-loader": "^12.6.0",
    "tailwind-config-viewer": "^1.7.2",
    "tailwindcss": "^3.0.22",
    "wait-on": "^6.0.0"
  }
}
