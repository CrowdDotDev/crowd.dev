{
  "name": "@crowd/snowflake-connectors",
  "scripts": {
    "start": "CROWD_TEMPORAL_TASKQUEUE=snowflakeConnectors SERVICE=snowflake-connectors tsx src/index.ts",
    "dev": "CROWD_TEMPORAL_TASKQUEUE=snowflakeConnectors SERVICE=snowflake-connectors LOG_LEVEL=trace tsx --watch src/index.ts",
    "dev:local": "set -a && . ../../../backend/.env.dist.local && . ../../../backend/.env.override.local && set +a && CROWD_TEMPORAL_TASKQUEUE=snowflakeConnectors SERVICE=snowflake-connectors LOG_LEVEL=trace tsx --watch src/index.ts",
    "lint": "npx eslint --ext .ts src --max-warnings=0",
    "format": "npx prettier --write \"src/**/*.ts\"",
    "format-check": "npx prettier --check .",
    "tsc-check": "tsc --noEmit"
  },
  "dependencies": {
    "@crowd/archetype-standard": "workspace:*",
    "@crowd/database": "workspace:*",
    "@crowd/logging": "workspace:*",
    "@crowd/archetype-worker": "workspace:*",
    "@crowd/snowflake": "workspace:*",
    "@crowd/temporal": "workspace:*",
    "@crowd/types": "workspace:*",
    "@aws-sdk/client-s3": "^3.700.0",
    "@temporalio/workflow": "~1.11.8",
    "tsx": "^4.7.1",
    "typescript": "^5.6.3"
  }
}
