{
  "name": "@crowd/integration-data-worker",
  "scripts": {
    "start": "SERVICE=integration-data-worker tsx src/main.ts",
    "start:debug:local": "set -a && . ../../../backend/.env.dist.local && . ../../../backend/.env.override.local && set +a && SERVICE=integration-data-worker LOG_LEVEL=trace tsx --inspect=0.0.0.0:9232 src/main.ts",
    "start:debug": "SERVICE=integration-data-worker LOG_LEVEL=trace tsx --inspect=0.0.0.0:9232 src/main.ts",
    "dev:local": "nodemon --watch src --watch ../../libs --ext ts --exec pnpm run start:debug:local",
    "dev": "nodemon --watch src --watch ../../libs --ext ts --exec pnpm run start:debug",
    "lint": "npx eslint --ext .ts src --max-warnings=0",
    "format": "npx prettier --write \"src/**/*.ts\"",
    "format-check": "npx prettier --check .",
    "tsc-check": "tsc --noEmit",
    "script:process-data": "SERVICE=script tsx src/bin/process-data.ts",
    "script:process-data-for-tenant": "SERVICE=script tsx src/bin/process-data-for-tenant.ts"
  },
  "dependencies": {
    "@crowd/common": "workspace:*",
    "@crowd/database": "workspace:*",
    "@crowd/integrations": "workspace:*",
    "@crowd/logging": "workspace:*",
    "@crowd/redis": "workspace:*",
    "@crowd/sqs": "workspace:*",
    "@crowd/tracing": "workspace:*",
    "@crowd/types": "workspace:*",
    "@crowd/feature-flags": "workspace:*",
    "@crowd/common_services": "workspace:*",
    "@crowd/telemetry": "workspace:*",
    "config": "^3.3.9",
    "tsx": "^4.7.1",
    "typescript": "^5.0.4"
  },
  "devDependencies": {
    "@types/config": "^3.3.0",
    "@types/node": "^18.16.3",
    "nodemon": "^2.0.22"
  }
}
