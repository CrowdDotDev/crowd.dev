{
  "name": "@crowd/data-sink-worker",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "node -r tsconfig-paths/register -r ts-node/register ./src/bin/api.ts --transpile-only",
    "start:api:dev": "nodemon --watch \"src/**/*.ts\" -e ts,json --exec \"ts-node --transpile-only ./src/bin/api.ts\"",
    "start:debug:local": "set -a && . ../../../backend/.env.dist.local && . ../../../backend/.env.override.local && set +a && SERVICE=data-sink-worker LOG_LEVEL=trace node --inspect=0.0.0.0:9233 -r tsconfig-paths/register -r ts-node/register ./src/bin/api.ts --transpile-only",
    "start:debug": "SERVICE=data-sink-worker LOG_LEVEL=trace node --inspect=0.0.0.0:9233 -r tsconfig-paths/register -r ts-node/register src/main.ts --transpile-only",
    "start:watch:local": "nodemon --watch src --ext ts --exec npm run start:debug:local",
    "start:watch": "nodemon --watch src --ext ts --exec npm run start:debug",
    "dev:local": "concurrently \"npm run start:watch:local\" \"../../scripts/watch_libs.sh --service data_sink_worker\"",
    "dev": "concurrently \"npm run start:watch\" \"../../scripts/watch_libs.sh --service data_sink_worker\"",
    "lint": "eslint --ext .ts src --max-warnings=0 --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "format-check": "prettier --check .",
    "tsc-check": "tsc --noEmit",
    "script:restart-failed-results": "node -r tsconfig-paths/register -r ts-node/register src/bin/restart-failed-results.ts --transpile-only",
    "script:restart-result": "node -r tsconfig-paths/register -r ts-node/register src/bin/restart-result.ts --transpile-only"
  },
  "dependencies": {
    "@crowd/alerting": "file:../../libs/alerting",
    "@crowd/common": "file:../../libs/common",
    "@crowd/conversations": "file:../../libs/conversations",
    "@crowd/database": "file:../../libs/database",
    "@crowd/integrations": "file:../../libs/integrations",
    "@crowd/logging": "file:../../libs/logging",
    "@crowd/opensearch": "file:../../libs/opensearch",
    "@crowd/redis": "file:../../libs/redis",
    "@crowd/sentiment": "file:../../libs/sentiment",
    "@crowd/sqs": "file:../../libs/sqs",
    "@crowd/types": "file:../../libs/types",
    "@cubejs-client/core": "^0.33.12",
    "@opensearch-project/opensearch": "^1.2.0",
    "@pm2/io": "^5.0.0",
    "@sendgrid/eventwebhook": "^7.7.0",
    "@sendgrid/mail": "^7.7.0",
    "aws-sdk": "^2.1413.0",
    "axios": "^1.4.0",
    "bcrypt": "^5.1.0",
    "bunyan-middleware": "^1.0.2",
    "cli-highlight": "^2.1.11",
    "config": "^3.3.9",
    "cors": "^2.8.5",
    "express-rate-limit": "^6.7.1",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.1",
    "lodash.clonedeep": "^4.5.0",
    "lodash.isequal": "^4.5.0",
    "lodash.mergewith": "^4.6.2",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.43",
    "node-fetch": "^2.6.12",
    "passport": "^0.6.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-slack": "^0.0.7",
    "pg": "^8.11.1",
    "pg-connection-string": "^2.6.1",
    "sanitize-html": "^2.11.0",
    "sequelize": "^6.32.1",
    "slack-block-builder": "^2.7.2",
    "socket.io": "^4.7.1",
    "stripe": "^10.0.0",
    "swagger-ui-dist": "^5.1.0",
    "unleash-client": "^4.0.2",
    "validator": "^13.9.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.59.2",
    "@typescript-eslint/parser": "^5.59.2",
    "concurrently": "^8.0.1",
    "eslint": "^8.39.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.8",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.0.4",
    "@types/config": "^3.3.0",
    "@types/node": "^18.16.3"
  }
}
